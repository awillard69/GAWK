msgid ""
"Examples:\n"
"\tgawk '{ sum += $1 }; END { print sum }' file\n"
"\tgawk -F: '{ print $1 }' /etc/passwd\n"
msgstr ""
"Samplaí:\n"
"\tgawk '{ sum += $1 }; END { print sum }' comhad\n"
"\tgawk -F: '{ print $1 }' /etc/passwd\n"

